{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "no-console": false,
    "variable-name": false,
    "trailing-comma": [
      false
    ],
    "max-line-length": false,
    "no-var-requires": false,
    "object-literal-sort-keys": false,
    "interface-name": false,
    "suppressImplicitAnyIndexErrors": true,
    "array-type": [
      true,
      "array"
    ]
  },
  "rulesDirectory": []
}
